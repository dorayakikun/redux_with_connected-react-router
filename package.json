    "eslint": "5.14.1",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "jest-junit": "6.3.0",
    "prettier": "1.16.4",
    "ts-loader": "5.3.3",
    "typescript": "3.3.3",
    "typescript-eslint-parser": "21.0.2",
    "webpack": "4.29.5",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.2.0"
