    "ts-jest": "^23.10.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
