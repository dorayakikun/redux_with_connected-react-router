  },
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@types/history": "^4.7.2",
    "@types/jest": "^23.3.11",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.0.32",
    "@types/redux-form": "^7.4.11",
    "ajv": "^5.5.2",
    "connected-react-router": "^4.5.0",
    "history": "^4.7.2",
    "jest": "^24.1.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
