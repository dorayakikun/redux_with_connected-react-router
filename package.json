  },
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "@types/history": "4.7.2",
    "@types/jest": "23.3.14",
    "@types/react": "16.8.4",
    "@types/react-dom": "16.8.2",
    "@types/react-redux": "6.0.13",
    "@types/react-router": "4.4.4",
    "@types/redux-form": "7.5.2",
    "ajv": "6.9.1",
    "connected-react-router": "4.5.0",
    "history": "4.7.2",
    "jest": "24.1.0",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-redux": "5.1.1",
    "react-router": "4.3.1",
