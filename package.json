    "prettier": "^1.14.3",
    "ts-loader": "^5.2.2",
    "typescript": "^3.1.3",
    "typescript-eslint-parser": "^20.0.0",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
